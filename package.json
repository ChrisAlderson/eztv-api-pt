{
  "name": "eztv-api-pt",
  "version": "1.0.0",
  "description": "A eztv api wrapper to get data from eztv.ag.",
  "dependencies": {
    "cheerio": "^0.20.0",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-plugin-transform-strict-mode": "^6.8.0",
    "babel-preset-es2015": "^6.6.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "babel index.es6 -o index.js",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "keywords": [
    "eztv", "torrent", "api", "popcorn"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-strict-mode"
    ]
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisAlderson/eztv-api-pt.git"
  }
}
